<% Station.first.albums.where('id < 10').find_each do |a| %>
	<tbody>
      <tr>
        <td><%= a.artist_id %></td>
        <td><%= a.name %></td>
        <td><%= Artist.find(a.artist_id).name %></td>
        <td><%= a.genre %></td>
        <td><%= a.released %></td>
      </tr>
    </tbody>
<% end %>